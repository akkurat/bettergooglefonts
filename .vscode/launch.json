    "version": "0.2.0",
    "configurations": [
        

        {
            "type": "pwa-node",
            "request": "launch",
            "name": "collect font infos",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/gf.js",
            "args": ["${workspaceFolder}/bettergooglefontsng/src/assets/"]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "collect questionnaire",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/conv-questionnaire.js",
            "args": ["${workspaceFolder}/bettergooglefontsng/src/assets/"]
        },
    ]
}